{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "epics_msi_substitution",
      "path": ".",
      "scope": "source.epics_msi_substitution",
      "file-types": [
        "sub",
        "subs",
        "substitutions"
      ],
      "injection-regex": "^((epics-)?msi-)?substitutions?$",
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm"
    }
  ],
  "metadata": {
    "links": {
      "repository": "https://github.com/minijackson/tree-sitter-epics-msi-substitution"
    },
    "version": "0.1.3",
    "license": "MIT",
    "description": "tree-sitter grammar for EPICS' .substitutions files",
    "authors": [
